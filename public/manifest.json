{
	"manifestVersion": 1,
	"id": "omnitheme",
	"name": "Build a Theme",
	"author": "coldenate",
	"repoUrl": "https://github.com/coldenate/build-a-theme",
	"enableOnMobile": true,
	"version": {
		"major": 0,
		"minor": 1,
		"patch": 1
	},
	"theme": ["light", "dark"],
